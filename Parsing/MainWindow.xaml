<Window x:Class="Parsing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parsing"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="StartParse" Content="Button" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="75" Click="StartParse_Click"/>
        <TextBox x:Name="tbOut" HorizontalAlignment="Left" Height="350" Margin="21,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="715" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"/>
        <Button x:Name="bt_test" Content="Button" HorizontalAlignment="Left" Margin="116,0,0,0" VerticalAlignment="Top" Width="75" Click="Bt_test_Click"/>

    </Grid>
</Window>
